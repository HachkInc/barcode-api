generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model User {
  email   String    @unique
  name    String?
  telegram_id String
  user_id Int       @id @default(autoincrement())
}
